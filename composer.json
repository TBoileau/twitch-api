{
    "name": "tboileau/twitch-api",
    "description": "Twitch API for PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TBoileau\\TwitchApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TBoileau\\TwitchApi\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "TBoileau",
            "email": "t-boileau@email.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/http-client": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "symfony/console": "^6.3"
    }
}
